                        {
                            "datePublished": "2022-09-30",
                            "claimReviewed": "Shark swimming in flooded street in Florida",
                            "url": "https://newschecker.in/fact-check/hurricane-batters-florida-old-edited-photo-of-shark-wading-through-flooded-street-viral-again/"
                        }
